{
	"name": "@labor-digital/asset-building-env-vuejs",
	"version": "5.0.0-rc.3",
	"description": "An Extension for the LABOR.digital asset-builder to build vue.js apps with zero config",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:labor-digital/asset-building-env-vuejs.git"
	},
	"scripts": {
		"postinstall": "patch-package || exit 0",
		"clean": "rimraf ./dist",
		"install-dev:compile": "cd ../../ && npm run build",
		"install-dev": "npm run install-dev:compile && npm i",
		"build": "npm run clean && tsc",
		"watch": "npm run clean && tsc --project ./tsconfig.watch.json --watch"
	},
	"dependencies": {
		"@labor-digital/helferlein": "^4.0.5",
		"@types/vue": "2.0.0",
		"lru-cache": "^6.0.0",
		"patch-package": "^6.4.7",
		"vue": "2.6.12",
		"vue-loader": "15.9.6",
		"vue-property-decorator": "9.1.2",
		"vue-router": "3.5.1",
		"vue-server-renderer": "2.6.12",
		"vue-style-loader": "4.1.3",
		"vue-template-compiler": "2.6.12",
		"webpack-node-externals": "2.5.2"
	},
	"devDependencies": {
		"@labor-digital/asset-building": "file:../../",
		"rimraf": "^3.0.2",
		"@types/express": "4.17.11",
		"@types/memory-fs": "0.3.3",
		"typescript": "4.2.3"
	},
	"peerDependencies": {
		"@labor-digital/asset-building": "^9.0.0"
	},
	"author": {
		"name": "LABOR.digital",
		"email": "info@labor.tools",
		"url": "https://www.labor.digital"
	},
	"contributors": [
		{
			"name": "Martin Neundorfer",
			"email": "code@neunerlei.eu",
			"url": "https://www.neunerlei.eu"
		}
	],
	"bugs": {
		"url": "https://github.com/labor-digital/asset-building-env-vuejs/issues"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/labor-digital/asset-building-env-vuejs",
	"keywords": [
		"webpack",
		"vue.js",
		"vue",
		"framework",
		"frontend",
		"hot-reload",
		"express",
		"interop",
		"dev-server",
		"assets",
		"typescript",
		"builder",
		"simple",
		"easy",
		"configuration"
	]
}
